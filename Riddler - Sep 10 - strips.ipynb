    "import random\n",
    "iters = 10000\n",
    "n = 0\n",
    "for _ in range(iters):\n",
    "    paper = [8.5,11]\n",
    "    while min(paper) > 1:\n",
    "        paper[random.choice([0,1])] -= 1\n",
    "        n += 1\n",
    "print(n/iters)"
   ]
